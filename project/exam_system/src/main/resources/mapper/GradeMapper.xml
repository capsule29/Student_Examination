<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.exam_system.mapper.GradeMapper">
    <resultMap id="GradeResultMap" type="com.example.exam_system.pojo.Grade">
        <result column="id" property="gradeId"></result>
        <result column="student_id" property="studentId"></result>
        <result column="course_id" property="courseId"></result>
        <result column="grade" property="myGrade"></result>
        <result column="paper_id" property="paperId"></result>
    </resultMap>
</mapper>