<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.exam_system.mapper.StudentMapper">
    <resultMap id="StudentResultMap" type="com.example.exam_system.pojo.Student">
        <result column="student_id" property="uId"></result>
        <result column="student_name" property="name"></result>
        <result column="student_password" property="studentPassword"></result>
        <result column="permission" property="permission"></result>
        <result column="class_id" property="classId"></result>
        <result column="state" property="state"></result>
    </resultMap>
</mapper>